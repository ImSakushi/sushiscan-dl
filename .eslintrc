{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"/*, "eslint-plugin-import"*/],
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "env": {
    "es2021": true,
    "node": true
  },
  "root": true,
  "rules": {
    "@typescript-eslint/no-explicit-any": 0
    /*"import/extensions": ["error", "always", { "ts": "never" }]*/
  }
}
